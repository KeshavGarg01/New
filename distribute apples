#include <iostream>
using namespace std;

int main()
{
   int t;
   cin>>t;
   for(int i=0; i<t; i++)
   {
       long long int n,k;
       cin>>n;//number of apples
       cin>>k;//number of boxes
       
       if((n/k)%k==0)
       {
           cout<<"NO"<<endl;
       }
       else
       cout<<"YES"<<endl;
   }
   
   return 0;
}
